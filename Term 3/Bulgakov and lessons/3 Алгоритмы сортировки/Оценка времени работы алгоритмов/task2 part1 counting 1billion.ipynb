    "#task 2.1\n",
    "\n",
    "# Для засечения времени\n",
    "import time\n",
    "\n",
    "#Считаем до 1.000.000.000\n",
    "count = 1000000000\n",
    "i = 0\n",
    "\n",
    "start = time.time()\n",
    "\n",
    "while i <= count:\n",
    "    i+=1\n",
    "\n",
    "end = time.time()\n",
    "\n",
    "time_spent = end - start\n",
    "\n",
    "print(\"Чтобы досчитать до 1 млрд моему пк нужно = \" + str(time_spent))\n"
